					<ul class="depth4">
						{%- for dep4 in dep3.depth4 %}
							{#- 4뎁스 link --------> #} {%- set d4_link %} {{- gnb.d1_link }}{{ dep2.d2_link }}{{ dep3.d3_link }}{{ dep4.d4_link }} {%- endset %}
							{#- 4뎁스 loop index --> #} {%- set d4_loop = loop.index %}
